---
import { Icon } from "astro-icon/components";
import Layout from "../layouts/Layout.astro";
import App from "../components/App.svelte";
---

<Layout
    title="Shopping Whale"
    description="Todo list with a whale svg"
    viewTransition={false}
>
    <App client:load>
        <Icon slot="icon" name="whale" width={500} height={500} />
        <Icon slot="spinner" name="spinner" width={500} height={500} />
    </App>
</Layout>
